class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        dict={}
        for i in range(len(nums)):
            res=target-nums[i]
            if res in dict:
                return (dict[res],i)
            else:
                dict[nums[i]]=i
        return (-1,-1)
